{
    "name": "bhittani/download",
    "license": "MIT",
    "description": "Zip downloader.",
    "keywords": ["zip", "download"],
    "homepage": "https://github.com/kamalkhan/download",
    "authors": [
        {
            "role": "developer",
            "name": "Kamal Khan",
            "email": "shout@bhittani.com",
            "homepage": "http://bhittani.com"
        }
    ],
    "support": {
        "source": "https://github.com/kamalkhan/download",
        "issues": "https://github.com/kamalkhan/download/issues"
    },
    "autoload": {
        "psr-4": {
            "Bhittani\\Download\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bhittani\\Download\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "platform": { "php": "5.6" }
    },
    "require": {
        "php": ">=5.6",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "<8"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
